<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Biotech</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class ="PatchOperationAddModExtension" MayRequire="ludeon.rimworld.biotech">
					<xpath>Defs/ThingDef[defName="Gun_ToxbombLauncher"]</xpath>
					<value>
						<li Class="SK_WeaponMastery.DefModExtension_Mastery">
							<gmclass>Launcher</gmclass>
						</li>
					</value>
				</li>
				<li Class ="PatchOperationAddModExtension" MayRequire="ludeon.rimworld.biotech">
					<xpath>Defs/ThingDef[defName="Flamebow"]</xpath>
					<value>
						<li Class="SK_WeaponMastery.DefModExtension_Mastery">
							<gmclass>Bow</gmclass>
						</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>