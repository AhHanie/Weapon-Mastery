<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Dusk Armory</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class ="PatchOperationAddModExtension">
					<xpath>/Defs/ThingDef[defName="DF_Dusk_Carbine"]</xpath>
					<value>
						<li Class="SK_WeaponMastery.DefModExtension_Mastery">
							<gmclass>assault rifle</gmclass>
						</li>
					</value>
				</li>
				<li Class ="PatchOperationAddModExtension">
					<xpath>/Defs/ThingDef[defName="DF_Dusk_Enforcer"]</xpath>
					<value>
						<li Class="SK_WeaponMastery.DefModExtension_Mastery">
							<gmclass>pistol</gmclass>
						</li>
					</value>
				</li>
				<li Class ="PatchOperationAddModExtension">
					<xpath>/Defs/ThingDef[defName="DF_Dusk_MAG"]</xpath>
					<value>
						<li Class="SK_WeaponMastery.DefModExtension_Mastery">
							<gmclass>lmg</gmclass>
						</li>
					</value>
				</li>
				<li Class ="PatchOperationAddModExtension">
					<xpath>/Defs/ThingDef[defName="DF_Dusk_SMG"]</xpath>
					<value>
						<li Class="SK_WeaponMastery.DefModExtension_Mastery">
							<gmclass>smg</gmclass>
						</li>
					</value>
				</li>
				<li Class ="PatchOperationAddModExtension">
					<xpath>/Defs/ThingDef[defName="DF_Dusk_Striker"]</xpath>
					<value>
						<li Class="SK_WeaponMastery.DefModExtension_Mastery">
							<gmclass>shotgun</gmclass>
						</li>
					</value>
				</li>
				<li Class ="PatchOperationAddModExtension">
					<xpath>/Defs/ThingDef[defName="DF_Dusk_Kris"]</xpath>
					<value>
						<li Class="SK_WeaponMastery.DefModExtension_Mastery">
							<gmclass>sword</gmclass>
						</li>
					</value>
				</li>
				<li Class ="PatchOperationAddModExtension">
					<xpath>/Defs/ThingDef[defName="DF_Dusk_Razor"]</xpath>
					<value>
						<li Class="SK_WeaponMastery.DefModExtension_Mastery">
							<gmclass>sword</gmclass>
						</li>
					</value>
				</li>
				<li Class ="PatchOperationAddModExtension">
					<xpath>/Defs/ThingDef[defName="DF_Dusk_Saw"]</xpath>
					<value>
						<li Class="SK_WeaponMastery.DefModExtension_Mastery">
							<gmclass>saw</gmclass>
						</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>