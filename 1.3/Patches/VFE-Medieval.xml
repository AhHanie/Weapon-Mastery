<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Factions Expanded - Medieval</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class ="PatchOperationAddModExtension">
					<xpath>Defs/ThingDef[defName="VFEM_Wine"]</xpath>
					<value>
						<li Class="SK_WeaponMastery.DefModExtension_Mastery">
							<blacklisted>true</blacklisted>
						</li>
					</value>
				</li>
				<li Class ="PatchOperationAddModExtension">
					<xpath>Defs/ThingDef[defName="VFEM_MeleeWeapon_Claymore"]</xpath>
					<value>
						<li Class="SK_WeaponMastery.DefModExtension_Mastery">
							<gmclass>bladed weapons</gmclass>
						</li>
					</value>
				</li>
				<li Class ="PatchOperationAddModExtension">
					<xpath>Defs/ThingDef[defName="VFEM_MeleeWeapon_HeavyMace"]</xpath>
					<value>
						<li Class="SK_WeaponMastery.DefModExtension_Mastery">
							<gmclass>blunt weapons</gmclass>
						</li>
					</value>
				</li>
				<li Class ="PatchOperationAddModExtension">
					<xpath>Defs/ThingDef[defName="VFEM_Bow_HeavyCrossbow"]</xpath>
					<value>
						<li Class="SK_WeaponMastery.DefModExtension_Mastery">
							<gmclass>crossbow</gmclass>
						</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>